{
  "name": "pddl-workspace",
  "version": "3.4.0",
  "description": "PDDL Workspace",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "mocha -- out/test/**/*.js",
    "pretest": "eslint ./src/**/*.ts && npm run compile",
    "build": "tsc",
    "compile": "tsc && npm run copyToDist",
    "copyToDist": "copyfiles --error --verbose --up 2 ./out/src/*.* ./out/src/**/*.* ./dist/",
    "prepack": "npm run test && npm run copyToDist",
    "watch": "tsc -w",
    "clean": "rmdir out /s && rmdir dist /s"
  },
  "keywords": [
    "pddl",
    "ai",
    "planning"
  ],
  "author": "Jan Dolejsi",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jan-dolejsi/pddl-workspace"
  },
  "devDependencies": {
    "typescript": "^3.8.3",
    "@types/node": "^13.9.1",
    "@types/nunjucks": "^3.1.3",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "@types/mocha": "^7.0.2",
    "@types/tmp": "^0.1.0",
    "@types/chai": "4.1.3",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "mocha": "^7.1.0",
    "copyfiles": "2.2.0",
    "nyc": "^15.0.0"
  },
  "dependencies": {
    "tmp-promise": "2.0.2",
    "nunjucks": "^3.2.0",
    "xml2js": "^0.4.23",
    "parse-xsd-duration": "^0.5.0",
    "vscode-uri": "^2.1.1"
  }
}
