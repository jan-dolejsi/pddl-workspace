{
  "name": "pddl-workspace",
  "version": "8.1.0",
  "description": "PDDL Workspace",
  "main": "dist/cjs/index.js",
  "types": "dist/cjs/index.d.ts",
  "scripts": {
    "test": "npx mocha -- out/test/**/*Test.js",
    "pretest": "npx eslint ./src/**/*.ts && npm run compile",
    "build:esm": "tsc -p ./tsconfig.esm.json",
    "build:cjs": "tsc -p ./tsconfig.cjs.json",
    "build": "npm run build:esm && npm run build:cjs",
    "compile": "tsc && npm run copyToDist",
    "copyToDist:esm": "copyfiles --error --verbose --up 2 \"./out/esm/src/**/*.*\" ./dist/esm",
    "copyToDist:cjs": "copyfiles --error --verbose --up 2 \"./out/cjs/src/**/*.*\" ./dist/cjs",
    "copyToDist": "npm run copyToDist:cjs && npm run copyToDist:esm",
    "prepack": "npm run test && npm run copyToDist",
    "watch": "tsc -w -p ./tsconfig.esm.json",
    "clean": "rmdir out /s && rmdir dist /s"
  },
  "keywords": [
    "pddl",
    "ai",
    "planning"
  ],
  "author": "Jan Dolejsi",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jan-dolejsi/pddl-workspace"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/types/index.d.ts",
        "default": "./dist/esm/index.mjs"
      },
      "require": {
        "types": "./dist/cjs/types/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "files": [
    "dist/**/*"
  ],
  "devDependencies": {
    "@types/chai": "4.3.1",
    "@types/chai-string": "1.4.2",
    "@types/mocha": "^9.1.0",
    "@types/node": "12.20.34",
    "@types/nunjucks": "^3.2.1",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "chai": "^4.3.6",
    "chai-string": "^1.5.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.13.0",
    "mocha": "^9.2.2",
    "nyc": "^15.1.0",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "nunjucks": "^3.2.3",
    "parse-xsd-duration": "^0.5.0",
    "vscode-uri": "^3.0.3",
    "xml2js": "^0.4.23"
  }
}
